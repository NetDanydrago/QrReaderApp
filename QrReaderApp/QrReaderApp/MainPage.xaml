<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="QrReaderApp.MainPage">
    <StackLayout>
        <Button x:Name="PickButton" Text="Pick Photo"  />
        <Button x:Name="TakeButton" Text="Take Photo"  />
        <Button x:Name="ScanButton" Text="Scan Photo"  />
        <Image x:Name="PhotoImage"  WidthRequest="200" HeightRequest="200" />
        <Label x:Name="TextQr"/>
    </StackLayout>

</ContentPage>
